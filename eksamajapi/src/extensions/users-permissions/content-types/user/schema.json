{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "dob": {
      "type": "date"
    },
    "father": {
      "type": "string"
    },
    "mother": {
      "type": "string"
    },
    "husband": {
      "type": "string"
    },
    "marital": {
      "type": "string"
    },
    "occupation": {
      "type": "string"
    },
    "gotra": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "jati": {
      "type": "string"
    },
    "cast": {
      "type": "string"
    },
    "isdivyang": {
      "type": "boolean",
      "default": false
    },
    "divyangdescription": {
      "type": "richtext"
    },
    "relationship": {
      "type": "string"
    },
    "privacy": {
      "type": "json"
    },
    "user_title": {
      "type": "string"
    },
    "mobile": {
      "type": "biginteger"
    },
    "addresses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::address.address",
      "mappedBy": "users"
    },
    "userrole": {
      "type": "enumeration",
      "enum": [
        "USER",
        "SUPER_ADMIN",
        "ADMIN"
      ]
    },
    "userstatus": {
      "type": "enumeration",
      "enum": [
        "APPROVED",
        "UNAPPROVED",
        "REJECTED",
        "BLOCKED",
        "PENDING",
        "ENGAGED",
        "SUSPENDED"
      ],
      "default": "PENDING"
    },
    "userid": {
      "type": "string"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "samaj": {
      "type": "string"
    },
    "merital_status": {
      "type": "string"
    },
    "Profession": {
      "type": "string"
    },
    "photos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "settingjson": {
      "type": "json"
    },
    "sentRequests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::connectionrequest.connectionrequest",
      "mappedBy": "sender"
    },
    "receivedRequests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::connectionrequest.connectionrequest",
      "mappedBy": "receiver"
    },
    "msgsernder": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "sender"
    },
    "msgreciver": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chat.chat",
      "mappedBy": "reciever"
    },
    "orgsku": {
      "type": "enumeration",
      "enum": [
        "TELI0001",
        "SEERVI0002",
        "DEMO0003"
      ]
    },
    "donners": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::donner.donner",
      "mappedBy": "donationreciever"
    },
    "qrimage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "auditlogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::auditlog.auditlog",
      "mappedBy": "changedby"
    },
    "orgs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::org.org",
      "mappedBy": "users"
    },
    "comunitymembers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::comunitymember.comunitymember",
      "mappedBy": "users_permissions_users"
    }
  }
}
